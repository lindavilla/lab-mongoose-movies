<h2>All Movies</h2>

<div>
    {{#each movies}}
   <ul>
        <li>
            <a href='/movies/{{this._id}}'>{{this.title}}</a>
        </li>
    </ul>
      <form action="/movies/{{this._id}}/delete" method="POST">
        <button>Delete Movie</button>
  </form>
    {{/each}}

   
    
</div>

<a href='/movies/new'>Create a New Movie</a>
